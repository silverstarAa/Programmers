class Solution {
  public boolean solution(String s) {
      if (s.length() != 4 && s.length() != 6) return false;
      
      String[] arr = s.split("");
      for (String a : arr) {
          boolean flag = false;
          for (int i = 0; i < 10; i++) {
              if (a.equals(String.valueOf(i))) {
                  flag = true;
                  break;
              }
          }
          
          if (!flag) return false;
      }
      return true;
  }
}
