SELECT 
    A.HOUR
    , NVL(B.CNT, 0)
FROM (
    SELECT
        (LEVEL - 1) AS HOUR
    FROM DUAL
    CONNECT BY LEVEL <= 24
) A
LEFT OUTER JOIN (
    SELECT 
        TO_CHAR(DATETIME, 'HH24') AS HOUR
        , COUNT(1) AS CNT
    FROM ANIMAL_OUTS
    GROUP BY TO_CHAR(DATETIME, 'HH24')
) B ON B.HOUR = A.HOUR
ORDER BY HOUR
;
